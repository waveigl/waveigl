{
  "crons": [
    {
      "path": "/api/moderation/reapply-timeouts",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/chat/poll",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/auth/unlink/process",
      "schedule": "*/1 * * * *"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase-url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase-anon-key",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase-service-key",
    "MERCADOPAGO_ACCESS_TOKEN": "@mercadopago-token",
    "TWITCH_CLIENT_ID": "@twitch-client-id",
    "TWITCH_CLIENT_SECRET": "@twitch-secret",
    "TWITCH_EVENTSUB_SECRET": "@twitch-eventsub-secret",
    "TWITCH_BOT_ACCESS_TOKEN": "@twitch-bot-token",
    "TWITCH_BOT_USER_ID": "@twitch-bot-user-id",
    "YOUTUBE_API_KEY": "@youtube-api-key",
    "YOUTUBE_CHANNEL_ID": "@youtube-channel-id",
    "GOOGLE_CLIENT_ID": "@google-client-id",
    "GOOGLE_CLIENT_SECRET": "@google-client-secret",
    "KICK_CLIENT_ID": "@kick-client-id",
    "KICK_CLIENT_SECRET": "@kick-client-secret",
    "KICK_CHANNEL_ID": "@kick-channel-id",
    "KICK_BROADCASTER_USER_ID": "@kick-broadcaster-user-id",
    "KICK_CHATROOM_ID": "@kick-chatroom-id",
    "DISCORD_BOT_TOKEN": "@discord-token",
    "DISCORD_GUILD_ID": "@discord-guild-id",
    "DISCORD_SUB_WEBHOOK_URL": "@discord-sub-webhook",
    "NOTIFY_UNREGISTERED_SUBS": "true",
    "INTERNAL_API_KEY": "@internal-api-key"
  }
}
